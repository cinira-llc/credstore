{
  "macOS Keychain": {
    "adapter": "CommandLine",
    "match": "command('security')",
    "actions": {
      "delete": [
        "delete-generic-password",
        "-s",
        "${service}",
        "-a",
        "${username}"
      ],
      "get": [
        "find-generic-password",
        "-s",
        "${service}",
        "-a",
        "${username}",
        "-w"
      ],
      "set": [
        "add-generic-password",
        "-s",
        "${service}",
        "-a",
        "${username}",
        "-w",
        "${password}"
      ]
    }
  },
  "Ubuntu Keyring": {
    "adapter": "CommandLine",
    "match": "command('secret-tool')",
    "actions": {
      "delete": [
        "clear",
        "service",
        "${service}",
        "username",
        "${username}"
      ],
      "get": [
        "lookup",
        "service",
        "${service}",
        "username",
        "${username}"
      ],
      "set": [
        "store",
        "--label",
        "${service}/${username}",
        "service",
        "${service}",
        "username",
        "${username}",
        "<${password}"
      ]
    }
  },
  "Windows Credential Manager": {
    "adapter": "WindowsCredentialManager",
    "match": "env('OS', /^Windows_/)"
  }
}